// filepath: [runner.yml](http://_vscodecontentref_/1)
name: Run Python Script Daily

on:
  schedule:
    - cron: "0 18 * * *"
  workflow_dispatch:

jobs:
  execute-python:
    runs-on: ubuntu-latest
    steps:
      - name: Checkout repository
        uses: actions/checkout@v3

      - name: Set up Python
        uses: actions/setup-python@v4
        with:
          python-version: "3.10"

      - name: Install dependencies
        run: |
          if [ -f [requirements.txt](http://_vscodecontentref_/2) ]; then pip install -r requirements.txt; fi

      - name: Run script
        env:
          TOKEN: ${{ secrets.TELEGRAM_TOKEN }}
          CHAT_ID: ${{ secrets.TELEGRAM_CHAT_ID }}
        run: python [scrapper.py](http://_vscodecontentref_/3)